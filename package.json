{
  "name": "tappay",
  "version": "2.0.1",
  "description": "TapPay Backend SDK",
  "license": "MIT",
  "repository": "https://github.com/Canner/tappay",
  "author": {
    "name": "wwwy3y3",
    "email": "wwwy3y3@gmail.com",
    "url": ""
  },
  "keywords": [
    ""
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "tslint --force --format verbose \"src/**/*.ts\" \"test/*\"",
    "prepublish": "npm run build",
    "build": "npm run clean && npm run lint && echo Using TypeScript && tsc --version && tsc -p ./tsconfig.build.json --pretty",
    "test": "mocha --compilers ts:ts-node/register --recursive \"test/**/*.spec.ts\"",
    "coverage": "nyc npm run test",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch"
  },
  "dependencies": {
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.43",
    "@types/node": "^9.4.6",
    "@types/supertest": "^2.0.3",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "mocha": "^5.0.1",
    "nyc": "^11.2.1",
    "prettier": "^1.5.2",
    "rimraf": "^2.0.0",
    "supertest": "^3.0.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.0.0",
    "tslint-config-prettier": "^1.1.0",
    "typescript": "^2.0.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.d.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text",
      "html",
      "lcov"
    ],
    "all": true
  }
}
